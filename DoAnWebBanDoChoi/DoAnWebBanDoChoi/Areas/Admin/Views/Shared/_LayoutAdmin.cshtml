<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>@ViewData["Title"] - Admin</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Heebo:wght@400;500;600;700&display=swap" rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="~/Admin/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet" asp-append-version="true">
    <link href="~/Admin/lib/tempusdominus/css/tempusdominus-bootstrap-4.min.css" rel="stylesheet" asp-append-version="true" />

    <!-- Customized Bootstrap Stylesheet -->
    <link href="~/Admin/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true">

    <!-- Template Stylesheet -->
    <link href="~/Admin/css/style.css" rel="stylesheet" asp-append-version="true">
</head>

<body>
    @if (TempData["Success"] != null)
    {
        <input type="hidden" id="temp-success-msg" value="@TempData["Success"]" />
    }
    @if (TempData["Error"] != null)
    {
        <input type="hidden" id="temp-error-msg" value="@TempData["Error"]" />
    }
    <div class="container-xxl position-relative bg-white d-flex p-0">
        <!-- Sidebar -->  

        @await Component.InvokeAsync("Sidebar")
        <!-- Content Start -->
        <div class="content">
            <!-- Navbar Start -->
            <nav class="navbar navbar-expand bg-light navbar-light sticky-top px-4 py-0">
                <a href="#" class="sidebar-toggler flex-shrink-0">
                    <i class="fa fa-bars"></i>
                </a>
            </nav>
         
			@RenderBody()
        
        </div>
      
       
    </div>

    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="https://esgoo.net/scripts/jquery.js"></script> 
    <script src="~/Admin/lib/chart/chart.min.js" asp-append-version="true"></script>
    <script src="~/Admin/lib/easing/easing.min.js" asp-append-version="true"></script>
    <script src="~/Admin/lib/waypoints/waypoints.min.js" asp-append-version="true"></script>
    <script src="~/Admin/lib/owlcarousel/owl.carousel.min.js" asp-append-version="true"></script>
    <script src="~/Admin/lib/tempusdominus/js/moment.min.js" asp-append-version="true"></script>
    <script src="~/Admin/lib/tempusdominus/js/moment-timezone.min.js" asp-append-version="true"></script>
    <script src="~/Admin/lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js" asp-append-version="true"></script>
    <script src="~/admin/js/alert.js"></script>
    <partial name="_ValidationScriptsPartial" />
    <!-- Template Javascript -->
    <script src="~/Admin/js/main.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>

</html>